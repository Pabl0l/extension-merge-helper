{
  "name": "merge-helper",
  "displayName": "Merge Helper",
  "description": "Merges duplicates and applies the latest versions of blocks/functions",
  "version": "0.0.1",
  "publisher": "JuanPabloOliveraMendoza",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pabl0l/extension-merge-helper"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Formatters",
    "Other"
  ],
  "keywords": [
    "merge",
    "replace",
    "duplicate",
    "refactor",
    "javascript",
    "python",
    "css"
  ],
  "icon": "icon.png",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "merge-helper.merge-javascript",
        "title": "ðŸ¥„ Merge JavaScript",
        "category": "Merge Helper"
      },
      {
        "command": "merge-helper.merge-css",
        "title": "ðŸ¥„ Merge CSS",
        "category": "Merge Helper"
      },
      {
        "command": "merge-helper.merge-python",
        "title": "ðŸ¥„ Merge Python",
        "category": "Merge Helper"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "merge-helper.merge-javascript",
          "group": "0_mergehelper",
          "when": "editorLangId == javascript || editorLangId == typescript || editorLangId == typescriptreact || editorLangId == javascriptreact"
        },
        {
          "command": "merge-helper.merge-css",
          "group": "0_mergehelper",
          "when": "editorLangId == css"
        },
        {
          "command": "merge-helper.merge-python",
          "group": "0_mergehelper",
          "when": "editorLangId == python"
        }
      ]
    }
  },
  "activationEvents": [
    "onCommand:merge-helper.merge-javascript",
    "onCommand:merge-helper.merge-css",
    "onCommand:merge-helper.merge-python"
  ],
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "node esbuild.js",
    "watch": "node esbuild.js --watch",
    "package": "node esbuild.js --production",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.10",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2"
  }
}
